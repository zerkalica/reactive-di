[ignore]
.*/dist/.*
.*/build/.*
.*/examples/.*
.*/node_modules/bab.*
.*/node_modules/lodash.*
.*/node_modules/array.*
.*/node_modules/is-.*
.*/node_modules/object.*
.*/node_modules/acorn.*

[libs]
lib/

[options]
munge_underscores=true
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
suppress_comment= \\(.\\|\n\\)*\\$FlowDisable
module.name_mapper='.*\(\.css\)' -> 'empty/object'
module.name_mapper='.*\(\.configloaderrc\)' -> 'empty/object'
module.name_mapper='^reactive-di/i/\(.*\)' -> '<PROJECT_ROOT>/i/\1'
module.name_mapper='^reactive-di/\(.*\)' -> '<PROJECT_ROOT>/src/\1'
